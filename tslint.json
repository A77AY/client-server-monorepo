{
    "defaultSeverity": "error",
    "extends": [],
    "jsRules": {},
    "rules": {
        "ordered-imports": [
            true,
            {
                "grouped-imports": true,
                "groups": [
                    { "name": "node_modules", "match": "^(?![.])", "order": 0 },
                    { "name": "current", "match": "^[.]", "order": 10 }
                ]
            }
        ]
    },
    "rulesDirectory": []
}
